    "firebase": "4.6.2",
    "lru-cache": "^4.1.1",
    "moment": "^2.17.1",
    "numeral": "^2.0.4",
    "route-cache": "0.4.3",
    "serve-favicon": "^2.4.5",
    "url-loader": "0.5.8",
